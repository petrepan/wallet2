      .accordion {
        background-color: #eee;
        color: #444;
        cursor: pointer;
        padding: 15px;
        width: 100%;
        border: none;
        text-align: left;
        outline: none;
        font-size: 15px;
        transition: 0.4s;
      }

      .current,
      .accordion:hover {
        background-color: #309030;
      }

      .accordion:after {
        content: '\002B';
        color: green;
        font-weight: bold;
        float: right;
        margin-left: 5px;
      }

      .current:after {
        content: "\2212";
      }

      .panel {
        padding: 0 18px;
        background-color: white;
        max-height: 0;
        overflow: hidden;
        transition: max-height 0.2s ease-out;
      }

      .navbar-toggler {
        background-color: #309030 !important;
      }

      .jumbotron {
        background-color: rgb(158, 163, 158);
      }

      .green-gradient {
        background-image: -webkit-linear-gradient(286deg, #90cc41, #21954a);
        background-image: linear-gradient(164deg, #90cc41, #21954a);
      }

      footer {
        background-color: #309030;
      }

      footer a {
        font-size: 14px !important;
      }

      .icon {
        font-size: 32px !important;
        color: #309030;
      }

      @media (min-width: 768px) {
        .bd-placeholder-img-lg {
          font-size: 3.5rem;
        }
      }

      @media (max-width: 968px) {
        .nav-item {
          padding-bottom: 10px;
        }

        a {
          display: inline-block !important;
        }
      }

    </style>
